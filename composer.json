{
    "name": "nepada/autocomplete-input",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Autocomplete text input for Nette forms.",
    "keywords": ["typeahead", "autocomplete", "form", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/autocomplete-input/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/application": "^3.0.2@dev",
        "nette/forms": "^3.0@dev",
        "nette/utils": "^3.1.3@dev"
    },
    "require-dev": {
        "nette/tester": "2.3.5",
        "mockery/mockery": "1.4.3",
        "phpstan/phpstan": "0.12.79",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "pepakriz/phpstan-exception-rules": "0.11.6",
        "phpstan/phpstan-nette": "0.12.14",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.3.0",
        "phpstan/phpstan-mockery": "0.12.12",
        "php-parallel-lint/php-parallel-lint": "1.2",
        "nepada/coding-standard": "7.3",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.5@dev",
        "nette/schema": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\AutocompleteInput\\": "src/AutocompleteInput/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
