{
    "name": "nepada/autocomplete-input",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Autocomplete text input for Nette forms.",
    "keywords": ["typeahead", "autocomplete", "form", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/autocomplete-input/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.4",
        "nette/application": "^3.1.4@dev",
        "nette/forms": "^3.1@dev",
        "nette/utils": "^3.2@dev || ^4.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.2",
        "mockery/mockery": "1.6.7",
        "phpstan/phpstan": "1.10.66",
        "phpstan/phpstan-strict-rules": "1.5.2",
        "phpstan/phpstan-nette": "1.2.9",
        "phpstan/phpstan-mockery": "1.1.2",
        "nepada/phpstan-nette-tester": "1.1.0",
        "spaze/phpstan-disallowed-calls": "3.1.2",
        "shipmonk/phpstan-rules": "2.11.3",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.14.0",
        "nette/di": "^3.0.6@dev",
        "nette/schema": "^1.0.3@dev",
        "nette/bootstrap": ">=3.1@dev",
        "nette/component-model": ">=3.0.2",
        "nette/http": ">=3.1.0@dev",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\AutocompleteInput\\": "src/AutocompleteInput/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
